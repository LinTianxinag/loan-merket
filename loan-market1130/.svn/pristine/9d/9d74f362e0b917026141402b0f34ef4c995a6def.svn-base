<script type="text/javascript">
    $(function(){
        $('#platname').on('blur',function () {
            var platname=$('#platname').val();
            if(platname==""){
                $('.reserr').text("不能为空");
            }else{
                $('.reserr').text("");
                $.ajax({
                    url:'/plat/namecheck',
                    type:'get',
                    dataType:'json',
                    data:{platname:platname},
                    success:function(data){
                        if(data.status==100){
                            $('.reserr').text("都说了不要重复");
                        }else{
                            $('.reserr').text("");
                        }
                    }
                })
            }
        })
        $('#rate').on('blur',function () {
            var rate=$('#rate').val();
            if(rate==""){
                $('.rateerr').text("不能为空");
            }else{
                $('.rateerr').text("");
            }
        })
    })

